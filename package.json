{
  "name": "crm-events-backend",
  "version": "1.0.0",
  "description": "Backend server for CRM Events system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install && python3 -m pip install -r requirements.txt",
    "train-rasa": "cd rasa && rasa train",
    "start-rasa": "cd rasa && rasa run --enable-api --cors \"*\"",
    "start-rasa-actions": "cd rasa && rasa run actions"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "firebase-admin": "^13.1.0",
    "fs-extra": "^11.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "puppeteer": "^18.2.1",
    "qrcode": "^1.5.3",
    "rimraf": "^3.0.2",
    "socket.io": "^4.7.2",
    "whatsapp-web.js": "^1.26.1-alpha.3",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2"
  }
}
